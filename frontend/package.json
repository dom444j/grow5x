{
  "name": "grow5x-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run security:check && vite build",
    "build:prod": "node build-production.js",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist/stats.html",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "preview": "vite preview",
    "preview:prod": "vite preview --mode production",
    "security:check": "node scripts/check-secrets.cjs",
    "prebuild": "npm run security:check",
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "axios": "^1.7.4",
    "lucide-react": "^0.539.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.52.2",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^4.12.0",
    "react-router-dom": "^6.30.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/react-helmet-async": "^1.0.3",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "openapi-fetch": "^0.14.0",
    "openapi-typescript": "^7.9.1",
    "postcss": "^8.4.41",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.9.2",
    "vite": "^5.2.0"
  }
}
